                "cow_id": "{{cow_id}}",
                "is_inseminated": true,
                "date_of_insemination": "2024-03-21",
                "insemination_count": 2,
                "lactation_number": 3
              }
            }
          }
        },
        {
          "name": "Monitor Pregnancy",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/cows/monitor_pregnancy/",
            "body": {
              "mode": "raw",
              "raw": {
                "farm_id": "{{farm_id}}",
                "cow_id": "{{cow_id}}",
                "pregnancy_date": "2024-03-21",
