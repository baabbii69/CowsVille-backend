          "request": {
            "method": "POST",
            "url": "{{base_url}}/cows/monitor_birth/",
            "body": {
              "mode": "raw",
              "raw": {
                "farm_id": "{{farm_id}}",
                "cow_id": "{{cow_id}}",
                "calving_date": "2024-03-21",
                "last_calving_date": "2023-03-21",
                "calf_sex": "M"
              }
            }
          }
        },
        {
          "name": "Get Records",
          "item": [
            {
              "name": "Pregnancy Records",
              "request": {
                "method": "GET",
                "url": "{{base_url}}/cows/pregnancy_records/",
                "query": [
                  {
                    "key": "farm_id",
                    "value": "{{farm_id}}"
                  },
                  {
                    "key": "cow_id",
                    "value": "{{cow_id}}"
                  }
                ]
              }
            },
            {
              "name": "Birth Records",
              "request": {
                "method": "GET",
                "url": "{{base_url}}/cows/birth_records/",
                "query": [
                  {
                    "key": "farm_id",
                    "value": "{{farm_id}}"
                  },
                  {
                    "key": "cow_id",
                    "value": "{{cow_id}}"
                  }
                ]
              }
            },
            {
              "name": "Heat Sign Records",
              "request": {
                "method": "GET",
                "url": "{{base_url}}/cows/heat_sign_records/",
                "query": [
                  {
                    "key": "farm_id",
                    "value": "{{farm_id}}"
                  },
                  {
                    "key": "cow_id",
                    "value": "{{cow_id}}"
                  }
                ]
              }
            },
            {
              "name": "Medical Records",
              "request": {
                "method": "GET",
                "url": "{{base_url}}/cows/medical_records/",
                "query": [
                  {
                    "key": "farm_id",
                    "value": "{{farm_id}}"
                  },
                  {
                    "key": "cow_id",
                    "value": "{{cow_id}}"
                  },
                  {
                    "key": "type",
                    "value": "all"
                  }
                ]
              }
            }
          ]
        },
        {
          "name": "Farmer Medical Assessment",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/cows/farmer_medical_assessment/",
            "body": {
              "mode": "raw",
              "raw": {
                "farm_id": "{{farm_id}}",
                "cow_id": "{{cow_id}}",
                "sickness_description": "Reduced appetite and lethargy"
              }
            }
          }
        }
      ]
