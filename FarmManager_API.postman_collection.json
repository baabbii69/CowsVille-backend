  },
  "item": [
    {
      "name": "Farms",
      "item": [
        {
          "name": "List Farms",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/farms/",
            "query": [
              {
                "key": "search",
                "value": "",
                "description": "Search farms by ID, owner name, or address"
              }
            ]
          }
        },
        {
          "name": "Create Farm",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/farms/",
            "body": {
              "mode": "raw",
              "raw": {
                "farm_id": "FARM001",
                "owner_name": "John Doe",
                "address": "Addis Ababa",
                "telephone_number": "+251912345678",
                "fertility_camp_no": 1,
                "total_number_of_cows": 10,
                "number_of_calves": 2,
                "number_of_milking_cows": 5,
                "total_daily_milk": 100,
                "type_of_housing": 1,
                "type_of_floor": 1,
                "main_feed": "Grass and hay",
                "rate_of_cow_feeding": 1,
                "source_of_water": 1,
                "rate_of_water_giving": 1,
                "farm_hygiene_score": 3
              }
            }
          }
        },
        {
          "name": "Change Farm's Doctor",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/farms/{{farm_id}}/change_doctor/",
            "body": {
              "mode": "raw",
              "raw": {
                "doctor_id": 1
              }
            }
          }
        }
      ]
    },
    {
      "name": "Cows",
      "item": [
        {
          "name": "List Cows",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/cows/",
            "query": [
              {
                "key": "farm_id",
                "value": "{{farm_id}}",
                "description": "Filter by farm ID"
              },
              {
                "key": "breed",
                "value": "",
                "description": "Filter by breed type"
              },
              {
                "key": "search",
                "value": "",
                "description": "Search by cow ID or breed name"
              }
            ]
          }
        },
        {
          "name": "Create Cow",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/cows/",
            "body": {
              "mode": "raw",
              "raw": {
                "is_deleted": false,
                "cow_id": "01",
                "age_in_days": 730,
                "sex": "F",
                "parity": 2,
                "body_weight": 450.5,
                "bcs": 3.5,
                "lactation_number": 2,
                "days_in_milk": 100,
                "average_daily_milk": 25.5,
                "cow_inseminated_before": true,
                "last_date_insemination": "2025-03-11",
                "number_of_inseminations": 2,
                "id_or_breed_bull_used": "HF-BULL-001",
                "last_calving_date": "2025-03-11",
                "farm": "01",
                "breed": 1,
                "gynecological_status": 1
              }
            }
          }
        },
        {
          "name": "Get Cows by Farm",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/cows/by_farm/",
            "query": [
              {
                "key": "farm_id",
                "value": "{{farm_id}}",
                "description": "The ID of the farm to filter cows"
              }
            ]
          }
        },
        {
          "name": "Record Heat Sign",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/cows/record_heat_sign/",
